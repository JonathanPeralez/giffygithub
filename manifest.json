{
  "name": "Giffy Github",
  "version": "1.0",
  "description": "Stop using boring pull request messages!",
  "page_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
      "https://github.com/*"
    ],
    "js": [
      "jQuery.js", "popup.js"
    ],
    "css": [
      "popup-style.css"
    ]
  }],
  "manifest_version": 2
}